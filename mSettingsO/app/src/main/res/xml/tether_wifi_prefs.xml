<!--To Customize TCT WiFi Tethering menu
 * add on 6897886
 * @author yucheng.luo
 * @Time 2018-09-01-->
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
                  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">

    <PreferenceCategory
        android:title="@string/wifi_tether_setting_title"
        android:persistent="false"/>

    <SwitchPreference
        android:key="enable_wifi_ap"
        android:title="@string/wifi_hotspot_checkbox_text"
        android:summary="@string/summary_two_lines_placeholder" />

    <Preference
        android:key="wifi_ap_ssid_and_security"
        android:title="@string/wifi_tether_configure_ap_text"
        android:persistent="false" />
    
    <!--Begin added by yucheng.luo for XR7031036 on 18-9-28-->
    <Preference
        android:key="wps_connect"
        android:title="@string/wifi_tether_wps_connect_title"
        android:summary="@string/wifi_tether_wps_connect_summary"
        android:persistent="false" />
    <!--End added by yucheng.luo for XR7031036 on 18-9-28-->

    <PreferenceCategory
        android:key="connected_category"
        android:title="@string/wifi_ap_connected_title"
        android:persistent="false"/>

    <!--[TCT-ROM][Unblock Function]Begin added by yucheng.luo for XR7051529 & 7054243 on 18-10-26-->
    <PreferenceCategory
        android:key="blocked_category"
        android:title="@string/wifi_ap_blocked_title"
        android:persistent="false"/>
    <!--[TCT-ROM][Unblock Function]End added by yucheng.luo for XR7051529 & 7054243 on 18-10-26-->

</PreferenceScreen>
